# Postgres
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/abundant_crm?schema=public"

# Authentication (required for local Google OAuth)
AUTH_SECRET=""
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""
# Optional override. Defaults to http://localhost:3000/api/auth/google/callback
GOOGLE_OAUTH_REDIRECT_URI=""

# Required for AI-assisted web research prefill
OPENAI_API_KEY=""
OPENAI_MODEL="gpt-4.1-mini"
# Optional dedicated model for Workbench (beta) narrative planning/execution extraction.
# If unset, the app falls back to OPENAI_MODEL then OPENAI_SEARCH_MODEL.
OPENAI_AGENT_MODEL=""
OPENAI_SEARCH_MODEL="gpt-4o-mini"

# Optional: if you want to fan out search results before extraction
SERPAPI_API_KEY=""

# Optional: reserved for future Google Docs / Drive service-account reads
GOOGLE_DOCS_SERVICE_ACCOUNT_JSON=""
